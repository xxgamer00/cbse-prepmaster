  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup": "node scripts/setup-project.js",
    "android": "npm run build && npx cap sync && npx cap open android",
    "build:android": "node scripts/build-android.js",
    "build:android-release": "cross-env NODE_ENV=production node scripts/build-android.js",
    "resources": "node scripts/generate-assets.js",
    "setup:dev": "node scripts/setup-dev.js",
    "test:app": "node scripts/test-app.js",
    "test:pwa": "node scripts/test-pwa.js",
    "deploy:dev": "node scripts/deploy.js development",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:prod": "node scripts/deploy.js production",
    "prepare-android": "npm run resources && npm run build:android",
    "prepare-release": "npm run resources && npm run build:android-release",
    "clean": "rimraf build/ android/app/build/ android/app/release/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "node scripts/analyze.js",
    "analyze:bundle": "source-map-explorer 'build/static/js/*.js'",
    "analyze:deps": "npx depcheck",
    "version:major": "node scripts/version.js major",
    "version:minor": "node scripts/version.js minor",
    "version:patch": "node scripts/version.js patch",
    "version:beta": "node scripts/version.js beta",
    "version:alpha": "node scripts/version.js alpha",
    "translations:init": "node scripts/manage-translations.js init",
    "translations:extract": "node scripts/manage-translations.js extract",
    "translations:validate": "node scripts/manage-translations.js validate",
    "translations:sync": "node scripts/manage-translations.js sync",
    "translations:add": "node scripts/manage-translations.js add-language",
    "assets:optimize": "node scripts/optimize-assets.js",
    "postinstall": "npm run setup"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.1",
    "@capacitor/resources": "^3.0.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.53.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lighthouse": "^11.3.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "puppeteer": "^21.5.2",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "svgo": "^3.0.4",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
